{
  "name": "sentinelbench",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:build": "vite build --watch",
    "dev:serve": "wrangler pages dev dist --d1 DB",
    "dev:all": "concurrently \"npm run dev:build\" \"npm run dev:serve\"",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "type-check": "tsc --noEmit --skipLibCheck",
    "check:exports": "node scripts/check-exports.js",
    "check:types": "node scripts/check-types.js",
    "health-check": "node scripts/health-check.js",
    "validate": "npm run type-check && npm run check:exports && npm run lint",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:all": "node scripts/run-tests.js",
    "test:security": "vitest run src/__tests__/security",
    "test:integration": "vitest run src/__tests__/integration",
    "test:unit": "vitest run src/__tests__/utils src/__tests__/components"
  },
  "dependencies": {
    "@types/canvas-confetti": "^1.9.0",
    "@types/matter-js": "^0.19.8",
    "@types/styled-components": "^5.1.34",
    "axios": "^1.7.9",
    "canvas-confetti": "^1.9.3",
    "matter-js": "^0.20.0",
    "otpauth": "^9.4.0",
    "qrcode.react": "^4.2.0",
    "react": "^18.3.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.3.1",
    "react-google-recaptcha": "^3.1.0",
    "react-router-dom": "^6.30.1",
    "recharts": "^2.15.0",
    "styled-components": "^6.1.14"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250129.0",
    "@eslint/js": "^9.17.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/react-google-recaptcha": "^2.1.9",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "jsdom": "^26.0.0",
    "jspdf": "^3.0.1",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.3.5",
    "vitest": "^3.0.2"
  }
}
